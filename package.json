{
  "name": "dpp-client",
  "version": "1.2.4",
  "description": "",
  "private": false,
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/swashapp/dpp-client.git"
  },
  "main": "build/main/index.js",
  "outDir": "build",
  "type": "commonjs",
  "scripts": {
    "test": "jest",
    "build": "tsc -p tsconfig.json"
  },
  "files": [
    "build/main/*",
    "!server/*",
    "!**/*.spec.*",
    "!**/*.test.*",
    "!**/*.json",
    "CHANGELOG.md",
    "LICENSE",
    "README.md"
  ],
  "dependencies": {
    "cross-fetch": "3.1.5",
    "ethers": "5.7.1",
    "jsonwebtoken": "8.5.1",
    "tsc": "^2.0.4",
    "typescript": "^4.9.5",
    "web3": "1.7.5"
  },
  "devDependencies": {
    "@types/node": "14.14.31",
    "@typescript-eslint/eslint-plugin": "4.15.2",
    "@typescript-eslint/parser": "4.15.2",
    "@uniswap/router-sdk": "1.0.5",
    "eslint-config-prettier": "8.1.0",
    "eslint-plugin-import": "2.24.0",
    "eslint-plugin-prettier": "3.3.1",
    "jest": "26.6.3",
    "swash-order-router": "github:swashapp/smart-order-router#e7fa858daa3ed19d7a21ef90d58cc7c5fc71b506",
    "ts-jest": "26.5.2"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.test\\.ts$",
    "transform": {
      ".+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
