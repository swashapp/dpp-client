{
  "name": "dpp-client",
  "version": "1.1.10",
  "description": "",
  "private": true,
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/swashapp/dpp-client.git"
  },
  "main": "build/main/index.js",
  "outDir": "build",
  "type": "commonjs",
  "scripts": {
    "test": "jest",
    "build": "tsc -p tsconfig.json"
  },
  "files": [
    "build/main/*",
    "!**/*.spec.*",
    "!**/*.json",
    "CHANGELOG.md",
    "LICENSE",
    "README.md"
  ],
  "dependencies": {
    "cross-fetch": "^3.1.5",
    "jsonwebtoken": "^8.5.1",
    "ethers": "^5.7.1",
    "web3": "^1.7.5"
  },
  "devDependencies": {
    "@types/node": "^14.14.31",
    "@typescript-eslint/eslint-plugin": "^4.15.2",
    "@typescript-eslint/parser": "^4.15.2",
    "cross-env": "^7.0.3",
    "cross-var": "^1.1.0",
    "eslint": "^7.20.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-import": "^2.24.0",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-unused-imports": "^1.1.1",
    "jest": "^26.6.3",
    "prettier": "^2.2.1",
    "shx": "^0.3.3",
    "ts-jest": "^26.5.2",
    "ts-loader": "^8.0.17",
    "ts-node": "^9.1.1",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.1.5",
    "@uniswap/router-sdk": "1.0.5",
    "@uniswap/v2-sdk": "github:swashapp/v2-sdk#c9b85f07a7dab046625c78789302d527819401cf",
    "swash-order-router": "github:swashapp/smart-order-router#3bf441508bb21167b2a864075bce763b33ee02f1"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.test\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
